{
  "name": "astro-backend-ts",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p src/tsconfig.json",
    "start": "node dist/server.js",
    "dev": "nodemon",
    "worker": "nodemon --exec \"ts-node -r tsconfig-paths/register\" src/jobs/worker.ts",
    "seed": "ts-node -r tsconfig-paths/register scripts/seed-data.ts",
    "seed:dev": "nodemon --exec \"ts-node -r tsconfig-paths/register\" scripts/seed-data.ts",
    "join-users": "ts-node -r tsconfig-paths/register scripts/join-users-to-room.ts",
    "add-virtual-participants": "ts-node -r tsconfig-paths/register scripts/add-virtual-participants.ts"
  },
  "dependencies": {
    "@socket.io/mongo-adapter": "^0.4.0",
    "@types/morgan": "^1.9.10",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^1.73.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.5.2",
    "express-winston": "^4.2.0",
    "ioredis": "^5.3.2",
    "joi": "^18.0.0",
    "jsonwebtoken": "^9.0.0",
    "livekit-server-sdk": "^2.13.2",
    "mongodb": "^6.18.0",
    "mongoose": "^8.17.1",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.9",
    "socket.io": "^4.7.1",
    "uuid": "^11.0.4",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.17",
    "@types/joi": "^17.2.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.4.1",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.0"
  }
}
